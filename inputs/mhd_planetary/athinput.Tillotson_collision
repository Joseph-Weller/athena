pfloor = 1.0e-10  # pressure floor
efloor       = 1.e-1  # specific internal energy floor (in erg/g)
eceiling     = 1.e15  # specific internal energy ceiling (in erg/g)

<problem>
#pamb          = 1.0e-9    # ambient pressure
damb          = 1.0e-9    # ambient density
